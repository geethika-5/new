#hello world
print("hello world")

#Hello World with tab
print("Hello world \t Hello world")

#Hello World with new line
print("Hello world \nHello world")

#Integer Input
a=int(input())
print(a)

#float input
a=float(input())
print('%.2f'%a)

#character input
a=input()
print(a)

#string input
a=input()
print(a)

#Ascii-1
a=input()
print(ord(a))

#Ascii-2
a=input()
print(char(a))

